<!DOCTYPE html>
<html ng-app="CustomerTicketApp">
<head>
    <meta charset="utf-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title>@ViewBag.Title - My ASP.NET Application</title>
    <link rel="stylesheet" href="~/Content/materialize.min.css">
    @*<link rel="stylesheet" href="~/Content/bootstrap.min.css">*@
    <link rel="stylesheet" href="~/Content/datatables.min.css">
    <link rel="stylesheet" href="~/Content/Site.css">
    <link rel="icon" href="./favicon.ico" type="image/x-icon">
</head>
<body ng-controller="CustomerTicketController">
    <header class="header">
        <div class="nav-container">
            <div class="left-nav">
                <img src="~/Content/Assets/LOGO.png" class="logo"/> 
                <div class="site-divider">
                </div>
                <div class="learn-container">
                    <div class="learn">Learn</div>
                </div>
            </div>
            <div class="right-nav right"> 
                <a class="link" href="">Sign in</a> 
            </div>
        </div>
    </header>

    @RenderBody()
    <script src="~/Scripts/jquery-3.7.0.min.js"></script>
    <script src="https://cdnjs.cloudflare.com/ajax/libs/Chart.js/4.4.1/chart.min.js" crossorigin="anonymous"></script>
    <script src="~/Scripts/datatables.min.js"></script>
    <script src="~/Scripts/angular.min.js"></script>
    <script src="~/Scripts/app/module.js"></script>
    <script src="~/Scripts/app/service.js"></script>
    <script src="~/Scripts/app/controller.js"></script>
</body>
</html>
